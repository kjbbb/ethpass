{
	"manifest_version": 2,
	"name": "Etherium Password Manager",
	"version": "0.1",
	"browser_action": {
		"default_icon": "lock_key.png",
		"default_popup": "index.html"
	},

	"background": {
		"scripts": ["background.js"]
	},

	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],

			"js": ["jquery-3.2.1.min.js", "content.js"]
		}
	]

}